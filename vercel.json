{
  "version": 2,
  "installCommand": "cd Front-end/XDIGIX-t/sys/backend && npm ci",
  "buildCommand": "cd Front-end/XDIGIX-t/sys/backend && npm run build",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "includeFiles": "Front-end/XDIGIX-t/sys/backend/dist/**"
    }
  }
}
